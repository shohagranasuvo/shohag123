#include <iostream>

using namespace std;

int main()
{ int a,b;
cout<<"Enter row and cul ";
  cin>>a>>b;
    int arr[a][b];



    for(int i=0; i<a;i++)
    {
        for(int j=0 ;j<b;j++)
        { cout<<"Enter value row "<<i+1<<" and cul "<<j+1<<" ";
            cin>>arr[i][j];

        }
    }

    cout<<" Your matrix"<<endl;
     for (int i=0 ;i<a;i++)
    {
        for (int j= 0; j<b ;j++)

        {
           cout<< arr[i][j]<<" ";

        }
        cout<<endl;
    }

    //transpose

    int tran [b][a];

    for (int i=0 ;i<a;i++)
    {
        for (int j= 0; j<b ;j++)

        {
            tran [j][i]=arr[i] [j];

        }
    }
cout<<"Transpose matrix is "<<endl;
    for (int i=0 ;i<a;i++)
    {
        for (int j= 0; j<b ;j++)

        {
           cout<< tran[i][j]<<" ";

        }
        cout<<endl;
    }





 return 0;}
