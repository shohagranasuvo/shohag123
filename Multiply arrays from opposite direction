#include<iostream>
using namespace std;

int main() {
    int arr1[3] = {1, 2, 3}, arr2[3] = {4, 5, 3}, arr3[3];
 int t=0;


 for(int i=0;i<3;i++)
{


    arr3[t++]=arr1[i]*arr2[2-i];

}

for(int i=0;i<3;i++)

{
    cout<<arr3[i]<<endl;
}
    return 0;
}


